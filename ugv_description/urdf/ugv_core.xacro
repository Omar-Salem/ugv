            <mass value="1.4688367113110694"/>
            <inertia ixx="0.000755" iyy="0.001163" izz="0.000755" ixy="0.0" iyz="0.0" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="-0.1095 0.1312 -0.0425" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ugv_description)/meshes/rear_right_wheel.stl" scale="0.001 0.001 0.001"/>
            </geometry>
