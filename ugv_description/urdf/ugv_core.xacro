        <collision>
            <origin xyz="0.13672 -0.122 -0.0409" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ugv_description)/meshes/rear_left_wheel.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="rear_right_wheel">
        <inertial>
            <origin xyz="0.0 -0.020000000000000018 0.0" rpy="0 0 0"/>
            <mass value="0.0956"/>
            <inertia ixx="0.000842" iyy="0.001263" izz="0.000842" ixy="-0.0" iyz="0.0" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0.13672 0.122 -0.0409" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ugv_description)/meshes/rear_right_wheel.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0.13672 0.122 -0.0409" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ugv_description)/meshes/rear_right_wheel.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="front_right_wheel">
        <inertial>
            <origin xyz="0.0 -0.020000000000000018 0.0" rpy="0 0 0"/>
            <mass value="0.0956"/>
            <inertia ixx="0.000842" iyy="0.001263" izz="0.000842" ixy="-0.0" iyz="0.0" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="-0.13672 0.122 -0.0409" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ugv_description)/meshes/front_right_wheel.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="-0.13672 0.122 -0.0409" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ugv_description)/meshes/front_right_wheel.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="lidar_link">
        <inertial>
            <origin xyz="-0.027115069985041318 3.5226945429821856e-06 -0.0020552178870618665" rpy="0 0 0"/>
            <mass value="0.7886321015037847"/>
            <inertia ixx="0.000269" iyy="0.000272" izz="0.000333" ixy="0.0" iyz="0.0" ixz="-1e-06"/>
        </inertial>
        <visual>
            <origin xyz="-0.19 -0.0 -0.22" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ugv_description)/meshes/lidar_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="-0.19 -0.0 -0.22" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ugv_description)/meshes/lidar_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <joint name="front_left_wheel_joint" type="continuous">
        <origin xyz="0.13672 0.122 0.0409" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="front_left_wheel"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <joint name="rear_left_wheel_joint" type="continuous">
        <origin xyz="-0.13672 0.122 0.0409" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="rear_left_wheel"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <joint name="rear_right_wheel_joint" type="continuous">
        <origin xyz="-0.13672 -0.122 0.0409" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="rear_right_wheel"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <joint name="front_right_wheel_joint" type="continuous">
        <origin xyz="0.13672 -0.122 0.0409" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="front_right_wheel"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <joint name="lidar_link_joint" type="fixed">
        <origin xyz="0.19 0.0 0.22" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="lidar_link"/>
    </joint>

    <link name="base_footprint"/>

    <joint name="base_footprint_joint" type="fixed">
        <parent link="base_link"/>
        <child link="base_footprint"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

</robot>
