            <mass value="0.7886321015037847"/>
            <inertia ixx="0.000269" iyy="0.000272" izz="0.000333" ixy="0.0" iyz="0.0" ixz="-1e-06"/>
        </inertial>
        <visual>
            <origin xyz="-0.43536 6e-05 -0.091" rpy="0 0 0"/>
            <geometry>
