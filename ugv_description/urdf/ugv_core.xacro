            <geometry>
                <mesh filename="file://$(find ugv_description)/meshes/lidar_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="front_right_wheel">
        <inertial>
            <origin xyz="-3.129242420740752e-10 -0.018799999999999678 2.9802320333782717e-10" rpy="0 0 0"/>
            <mass value="1.4688367113110694"/>
            <inertia ixx="0.000755" iyy="0.001163" izz="0.000755" ixy="0.0" iyz="0.0" ixz="-0.0"/>
        </inertial>
        <visual>
            <origin xyz="-0.4007 0.1312 -0.0425" rpy="0 0 0"/>
            <geometry>
