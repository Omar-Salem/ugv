<?xml version="1.0" ?>
<robot name="ugv" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="$(find ugv_description)/urdf/materials.xacro"/>

    <link name="base_link">
        <inertial>
            <origin xyz="0.25539221363619424 -0.008191452128420306 0.07740258559841702" rpy="0 0 0"/>
            <mass value="1.8582255296411472"/>
            <inertia ixx="0.018307" iyy="0.036178" izz="0.054" ixy="-3e-06" iyz="-0.0" ixz="-5e-06"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ugv_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ugv_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="lidar_link">
